;
; Click Board Manifest Source for
; DIGI POT Click
; https://www.mikroe.com/digipot-click
;

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = digipot

; driver string
[string-descriptor 2]
string = mcp4161-103

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
mode=0
max-speed-hz=500000
protocol = 1

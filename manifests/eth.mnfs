;
; Click Board Manifest Source for
; ETH Click
; https://www.mikroe.com/eth-click
;

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1
reset-gpio-state = 2
int-gpio-state = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = eth

; driver string
[string-descriptor 2]
string = enc28j60

# -----------------------------------------------------------
#            Manifest Device Descriptor Section
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 1
mode=0
max-speed-hz=16000000
irq = 1
irq-type = 2
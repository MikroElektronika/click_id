;
; Click Board Manifest Source for
; THERMO Click
; https://www.mikroe.com/thermo-click
; CONFIG_MAXIM_THERMOCOUPLE

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = thermo

; driver string
[string-descriptor 2]
string = max31855k

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
mode=0
max-speed-hz=1000000
protocol = 1

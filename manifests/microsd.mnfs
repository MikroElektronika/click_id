;
; Click Board Manifest Source for
; microSD Click
; https://www.mikroe.com/microsd-click
;

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = microsd

; driver string
[string-descriptor 2]
string = mmc_spi

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
mode=0
max-speed-hz=50000000
protocol = 1
;
; Click Board Manifest Source for
; Smoke Click
; https://www.mikroe.com/smoke-click
;

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1
num-devices = 1
int-gpio-state = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = smoke

; driver string
[string-descriptor 2]
string = max30105

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 2
reg = 0x57
irq = 1
irq-type = 2

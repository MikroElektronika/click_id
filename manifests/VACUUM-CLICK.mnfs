;
; Click Board Manifest Source for
; VACUUM Click
; https://www.mikroe.com/vacuum-click
; CONFIG_SENSORS_MCP3021

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = vacuum

; driver string
[string-descriptor 2]
string = mcp3221

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 2
reg = 0x4d

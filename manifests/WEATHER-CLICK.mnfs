
; Click Board Manifest Source for
; WEATHER Click
; https://www.mikroe.com/weather-click
; CONFIG_BMP280_I2C

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = weather

; driver string
[string-descriptor 2]
string = bme280

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 2
reg = 0x76

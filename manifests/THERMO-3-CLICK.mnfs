;
; Click Board Manifest Source for
; Thermo 3 Click
; https://www.mikroe.com/thermo-3-click
; CONFIG_SENSORS_TMP102

# -----------------------------------------------------------
#            Manifest Header
# -----------------------------------------------------------
[manifest-header]
version-major = 0
version-minor = 1
click-string-id = 1

# -----------------------------------------------------------
#            Manifest String Descriptors
# -----------------------------------------------------------
; click string (id can't be 0)
[string-descriptor 1]
string = thermo3

; driver string
[string-descriptor 2]
string = tmp102

# -----------------------------------------------------------
#            Manifest Device Descriptors
# -----------------------------------------------------------
; device descriptor
[device-descriptor 1]
driver-string-id = 2
protocol = 2
reg = 0x48

[manifest-header]
version-major = 0
version-minor = 1

[interface-descriptor]
vendor-string-id = 1
product-string-id = 2

[string-descriptor 1]
string = MIKROE

[string-descriptor 2]
string = SHT1x 

[mikrobus-descriptor]
pwm-state = 4
int-state = 1
rx-state = 7
tx-state = 7
scl-state = 3
sda-state = 3
mosi-state = 5
miso-state = 5
sck-state = 5
cs-state = 5
rst-state = 2
an-state = 1

[device-descriptor 1]
driver-string-id = 3
protocol = 0xfe
reg = 0
gpio-link = 1

[string-descriptor 3]
string = sht11

[property-descriptor 1]
name-string-id = 4
type = 0x02
value = <2 3>

[string-descriptor 4]
string = gpio-link

[property-descriptor 2]
name-string-id = 5
type = 0x03
value = <4>

[string-descriptor 5]
string = clk

[property-descriptor 3]
name-string-id = 6
type = 0x03
value = <5>

[string-descriptor 6]
string = data